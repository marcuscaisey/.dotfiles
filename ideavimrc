source ~/.vimrc
set surround
set multiple-cursors
set commentary
set clipboard=ideaput
set ideajoin
set ReplaceWithRegister
set argtextobj
set textobj-entire

nmap [<space> :action EditorStartNewLineBefore<cr>
nmap ]<space> :action EditorStartNewLine<cr>
nmap <c-l> :action ExpressionTypeInfo<cr>
nmap <c-k> :action ShowErrorDescription<cr>
nmap [e :action GotoPreviousError<cr>
nmap ]e :action GotoNextError<cr>
nmap [c :action VcsShowPrevChangeMarker<cr>
nmap ]c :action VcsShowNextChangeMarker<cr>
